{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
